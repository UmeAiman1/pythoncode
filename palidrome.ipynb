    "n = int(input(\"enter a number\"))\n",
    "copy = n; \n",
    "num =0\n",
    "while n != 0:\n",
    "    rem = n %10;\n",
    "    num = (num*10)+rem\n",
    "    n //=10\n",
    "if num == copy:\n",
    "    print(\"palindrom\")\n",
    "else:\n",
    "    print(\"not a palindrom\")\n"
   ]
